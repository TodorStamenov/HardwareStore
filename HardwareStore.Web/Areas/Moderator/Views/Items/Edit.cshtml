@model ItemFormViewModel

@{
    ViewBag.Title = "Edit Item";
}

<div class="row">
    <div class="col-md-offset-2 col-md-8">
        <h2 class="col-md-offset-2">@ViewBag.Title</h2>
        @using (Html.BeginForm("Edit", "Items", new { area = WebConstants.ModeratorArea }, FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
        {
            @Html.Partial("_ItemForm", Model)
        }
    </div>
</div>